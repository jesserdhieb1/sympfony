{% extends 'base.html.twig' %}

{% form_theme formEtudiant'bootstrap_5_layout.html.twig' %}
{% block body %}

<div class="example-wrapper container col-6 ">
    {{ form_start(formEtudiant)  }}
    {{ form_row(formEtudiant.nom)  }}
    {{ form_row(formEtudiant.prenom)  }}
    {{ form_row(formEtudiant.age)  }}
    {{ form_row(formEtudiant.cin)  }}
    <button type="submit" class="btn btn-info">Enregistrer</button>
    <a  href="{{path('CRUD')}}" class="btn btn-warning" >retour</a>
    {{ form_end(formEtudiant)  }}
</div>

{% endblock %}